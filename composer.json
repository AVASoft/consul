{
  "name": "avasoft/consul",
  "type": "library",
  "description": "API Consul for AVASoft Market",
  "homepage": "https://avasoft.ru",
  "license": "MIT",
  "authors": [
    {
      "name": "Viktor Nazarov",
      "email": "viktor@avasoft.ru"
    }
  ],
  "require": {
    "php": ">=8.0",
    "psr/log": "^1.0",
    "ext-apcu": "*",
    "ext-curl": "*",
    "ext-yaml": "*",
    "ext-json": "*",
    "symfony/polyfill-php73": "^1.11",
    "symfony/polyfill-php80": "^1.15"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Avasoft\\Component\\Consul\\": "src"
    },
    "exclude-from-classmap": [
      "/tests/"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Avasoft\\Component\\Consul\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev"
}
